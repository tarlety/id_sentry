{
  "name": "id-sentry",
  "productName": "ElectronIDSentry",
  "version": "1.0.0",
  "description": "Simple Application to Record ID",
  "repository": "",
  "main": "src/main.js",
  "scripts": {
    "update:commit": "echo \"module.exports={number: '`git log --format='%H' -n 1 | cut -c1-8`'\", > commit.js ; echo \"developing: '`test -z \"\\`git status -s\\`\" && echo false || echo true`'\", >> commit.js ; echo \"date: '`git log -1 --format=%cd`'}\" >> commit.js",
    "start": "npm run update:commit && electron .",
    "package:linux": "npm run update:commit && electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:win": "npm run update:commit && electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=res/icon.png"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "electron-store": "^5.1.1",
    "jquery": "^3.4.1"
  }
}
